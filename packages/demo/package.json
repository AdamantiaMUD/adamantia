{
    "name": "@adamantiamud/demo",
    "version": "0.0.1-alpha.1",
    "description": "Sample MUD to demonstrate the basic concepts of the AdamantiaMUD game engine",
    "author": "Bill Parrott (https://chimericdream.com)",
    "license": "MIT",
    "repository": "github:adamantiamud/adamantia",
    "bugs": "https://github.com/AdamantiaMUD/adamantia/issues",
    "keywords": [],
    "homepage": "https://github.com/AdamantiaMUD/adamantia/tree/main/packages/demo#readme",
    "type": "module",
    "engines": {
        "node": ">=20"
    },
    "packageManager": "pnpm@9.0.2",
    "scripts": {
        "lint": "bunx eslint src/**/*",
        "lint:fix": "bunx eslint src/**/* --fix",
        "lint:quiet": "bunx eslint src/**/* --quiet",
        "postlint:fix": "bunx prettier --write src",
        "start": "bun ./src/server.ts",
        "type-check": "tsc --noEmit",
        "type-check:watch": "pnpm run type-check -- --watch"
    },
    "main": "src/server.ts",
    "dependencies": {
        "@adamantiamud/core": "workspace:*",
        "commander": "12.0.0"
    }
}
