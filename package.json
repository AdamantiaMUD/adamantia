{
    "name": "@adamantiamud/adamantia-demo",
    "version": "0.0.1-alpha.1",
    "description": "Sample MUD to demonstrate the basic concepts of the AdamantiaMUD game engine",
    "author": "Bill Parrott (https://chimericdream.com)",
    "license": "MIT",
    "repository": "github:adamantiamud/adamantia-demo",
    "bugs": "https://github.com/adamantiamud/adamantia-demo/issues",
    "type": "module",
    "types": "index.d.ts",
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "build": "tsc",
        "postbuild": "node scripts/copy-assets.js",
        "prestart": "npm run build",
        "start": "node ./build/server.js",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch"
    },
    "main": "build/server.js",
    "dependencies": {
        "@adamantiamud/adamantia-core": "file:../adamantia-core",
        "commander": "3.0.2"
    },
    "devDependencies": {
        "@chimericdream/eslint-config": "4.1.0",
        "@chimericdream/eslint-config-typescript": "3.0.0",
        "@types/fs-extra": "8.0.0",
        "@types/node": "12.7.8",
        "@typescript-eslint/eslint-plugin": "2.3.1",
        "@typescript-eslint/parser": "2.3.1",
        "eslint": "6.5.0",
        "fs-extra": "8.1.0",
        "typescript": "3.6.3"
    }
}
