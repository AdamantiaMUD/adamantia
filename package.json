{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "description": "Gatsby 1.0 starter",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:js && npm run lint:css",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:md": "remark .",
    "lint:js": "xo",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "graphql-codegen --config ./graphql-config.yml",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@graphql-codegen/add": "1.8.3",
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.14",
    "@types/react-redux": "7.1.5",
    "@types/semantic-ui": "^2.2.7",
    "babel-eslint": "10.0.3",
    "change-case": "3.1.0",
    "codeclimate-test-reporter": "0.5.1",
    "core-js": "3.4.1",
    "disqus-react": "1.0.7",
    "gatsby": "2.17.13",
    "gatsby-link": "2.2.23",
    "gatsby-plugin-google-analytics": "2.1.26",
    "gatsby-plugin-manifest": "2.2.27",
    "gatsby-plugin-offline": "3.0.20",
    "gatsby-plugin-sharp": "2.2.38",
    "gatsby-plugin-typescript": "2.1.17",
    "gatsby-remark-autolink-headers": "2.1.17",
    "gatsby-remark-copy-linked-files": "2.1.29",
    "gatsby-remark-images": "3.1.30",
    "gatsby-remark-prismjs": "3.3.22",
    "gatsby-source-filesystem": "2.1.36",
    "gatsby-transformer-json": "2.2.17",
    "gatsby-transformer-remark": "2.6.34",
    "gatsby-transformer-sharp": "2.3.4",
    "gray-matter": "4.0.2",
    "lodash": "4.17.15",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.3",
    "react-scripts": "3.2.0",
    "redux-devtools-extension": "2.13.8",
    "semantic-ui-react": "0.88.1",
    "slash": "3.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addon-notes": "5.2.6",
    "@storybook/addon-options": "5.2.6",
    "@storybook/react": "5.2.6",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.147",
    "@types/node": "12.12.7",
    "@types/react-test-renderer": "16.9.1",
    "@types/storybook__react": "4.0.2",
    "@types/webpack-env": "1.14.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.6.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-config-xo-react": "0.20.0",
    "eslint-plugin-flowtype": "4.4.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "flat": "5.0.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "mkdirp": "0.5.1",
    "plop": "2.5.3",
    "raw-loader": "3.1.0",
    "react-test-renderer": "16.11.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-recommended": "3.0.3",
    "storybook-readme": "5.0.8",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "ts-jest": "24.1.0",
    "ts-lint": "4.5.1",
    "xo": "0.25.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "gatsby",
    "starter"
  ],
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/",
      "/templates/",
      "/.cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "node tools/update-post-date.js",
      "remark -o",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "xo": {
    "extends": "xo-react/space",
    "space": true,
    "rules": {
      "no-case-declarations": 0,
      "default-case": 0
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
